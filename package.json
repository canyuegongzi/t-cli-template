  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.17.56",
    "@types/socket.io": "2.1.2",
    "@types/supertest": "^2.0.7",
    "@types/ws": "6.0.1",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "@vercel/ncc": "^0.28.3",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "typescript": "^3.9.5"
