  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.12",
    "@types/ws": "^8.5.4",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "7.1.0",
