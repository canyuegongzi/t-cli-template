    "class-validator": "^0.9.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "ejs": "^2.6.1",
    "exceljs": "^4.1.1",
    "express-session": "^1.16.1",
    "install": "^0.13.0",
    "js-base64": "^2.5.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "multer": "^1.4.1",
    "mysql": "^2.18.1",
    "nestjs-redis": "^1.2.3",
    "nodemailer": "^6.4.2",
