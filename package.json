  "scripts": {
    "build": "webpack --config ./script/config/webpack.common.ts --env production",
    "commit": "cz",
    "lint": "eslint --config ./.eslintrc.js . --ext .js,.jsx,.ts,.tsx,.vue",
    "lint:fix": "eslint --config ./.eslintrc.js . --ext .js,.jsx,.ts,.tsx,.vue --fix",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "push": "cz && standard-version",
    "release": "standard-version",
    "start": "cross-env webpack serve --config ./script/config/webpack.common.ts --env development",
    "mock": "cross-env webpack serve --config ./script/config/webpack.common.ts --env development",
