    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "ejs": "^3.1.8",
    "exceljs": "^4.3.0",
    "express-session": "^1.17.3",
    "install": "^0.13.0",
    "ioredis": "^5.3.0",
