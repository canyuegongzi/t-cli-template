  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p tsconfig.build.json",
    "build:min": "ts-node ./scripts/miniBuild.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{tsx,ts}\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "scan": "ts-node ./scripts/scanPackage.ts"
  },
  "dependencies": {
    "@nest-middlewares/express-session": "^7.0.1",
